<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vogella.android.constructormodel">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Voiture.Activity.FirstActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Voiture.Activity.MainActivity" />
        <activity android:name=".Voiture.Activity.SecondActivity" />
        <activity android:name=".Voiture.Activity.MotoActivity" />
        <activity android:name=".Voiture.Activity.MotoActivitySecond"></activity>
    </application>

</manifest>